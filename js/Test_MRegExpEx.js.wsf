    <job id="Test_RegExp_ParamsList_Replace">
        <script language="jscript" src="MRegExp.js" />
        <script language="jscript" src="MRegExpEx.js" />
        <script language="vbscript">
            Function GetSourceString()
                GetSourceString = InputBox("SourceString")
            End Function
            Function GetReplaceString()
                GetReplaceString = InputBox("ReplaceString")
            End Function
            Function GetPattern()
                GetPattern = InputBox("Pattern")
            End Function
            Function GetIgnoreCase()
                GetIgnoreCase = (MsgBox("IgnoreCase", vbYesNo) = vbYes)
            End Function
            Function GetGlobalMatch()
                GetGlobalMatch = (MsgBox("GlobalMatch", vbYesNo) = vbYes)
            End Function
            Function GetMultiLine()
                GetMultiLine = (MsgBox("MultiLine", vbYesNo) = vbYes)
            End Function
        </script>
        <script language="jscript">
            function GetReplacePatterns() {
                var ReplacePatterns = new Array();
                while (true) {
                    var ReplaceString = GetReplaceString();
                    if (ReplaceString == null) { break; }
                    if (ReplaceString == "") { break; }
                    
                    var Pattern = GetPattern();
                    if (Pattern == null) { break; }
                    if (Pattern == "") { break; }
                    
                    ReplacePatterns.push(ReplaceString + "\t" +Pattern);
                }
                return ReplacePatterns;
            }
        </script>
        <script language="jscript">
            Test_RegExp_ParamsList_Replace(
                GetSourceString(),
                GetReplacePatterns(),
                GetIgnoreCase(),
                GetGlobalMatch(),
                GetMultiLine());
        </script>
    </job>
